@inject UsuariosService UsuariosService

<RadzenPanelMenu Multiple="false" Match="NavLinkMatch.All">
    @foreach (var menu in lst)
    {
        <Menu Permiso="menu" />
    }
</RadzenPanelMenu>

@code {
    List<Permiso> lst = new();

    protected override async Task OnInitializedAsync()
    {
        lst = await UsuariosService.GetMenu();
    }
}
